generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  ID       Int    @id @default(autoincrement())
  UserID   String @unique @map("user_id")
  Username String @map("username")
}
